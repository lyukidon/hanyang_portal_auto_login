{
    "name": "Auto COVID19 Self Check for Hanyang",

    "version": "1.0.0",

    "manifest_version": 2,

    "icons": {
      "128": "./img/128.png",
      "48": "./img/48.png",
      "16": "./img/16.png"
    },

    "browser_action": {
        "default_popup": "Popup.html"
    },

    "content_scripts": [
        {
          "run_at": "document_idle",
          "matches": ["https://portal.hanyang.ac.kr/*"],
          "js": ["./src/Content.js"]
        }
      ]
}